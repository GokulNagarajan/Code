package com.mindtree.entity;
public class User {
	public String name;
	public String email;
	public int age;
	public User()
	{
		this.name = "";
		this.email = "";
		this.age = 0;
	}
	public User(String name, String email, int age) {
		super();
		this.name = name;
		this.email = email;
		this.age = age;
	}
	
}