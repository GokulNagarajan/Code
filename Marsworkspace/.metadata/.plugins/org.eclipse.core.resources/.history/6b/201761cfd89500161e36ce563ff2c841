function Name(val) {
	var x=val;
	if(x.length<6)
	{
		alert("Name not valid length");
		return false;
	}
}
function Password(val) {
	var x=val;
	if(x.length<8)
	{
		alert("Password not valid length");
		return false;
	}
}
function Email(Email) {
	var x=val;
	
	if(x.length<6)
	{
		alert("Email not valid");
		return false;
	}
}
function Age(val) {
	if(val<17)
	{
		alert("Age shoulbe above 18");
		return false;
	}
}
function Country(val) {
	if(val==-1)
	{
